{
  "$jason": {
    "head": {
      "title": "Profile View",
      "actions": {
        "$load": {
          "type": "$render"
        }
      },
      "styles": {
        "@": "https://raw.githubusercontent.com/Muran-Hu/SDUI/master/jasonelle/examples/styles.json"
      },
      "templates": {
        "body": {
          "sections": [
            {
              "items": [
                {
                  "type": "label",
                  "text": "How many accidents and violations have you had?",
                  "style": {
                    "padding": 30,
                    "spacing": 20,
                    "align": "center",
                    "corner_radius": "5",
                    "background": "rgba(f,f,f,f)",
                    "color": "#148554",
                    "font": "HelveticaNeue",
                    "size": 40
                  }
                }
              ]
            }
          ]
        }
      }
    }
  }
}
