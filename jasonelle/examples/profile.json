{
  "$jason": {
    "head": {
      "title": "Profile View",
      "actions": {
        "$load": {
          "type": "$render"
        }
      },
      "styles": {
        "@": "https://raw.githubusercontent.com/jasonelle/docs/develop/examples/jasonette/apps/beerjs/includes/styles.json"
      },
      "templates": {
        "body": {
          "sections": [
            {
              "items": [
                {
                  "type": "label",
                  "text": "{{$global.person.about}}",
                  "class": "color-dark-1, font-helvetica-neue"
                }
              ]
            }
          ]
        }
      }
    }
  }
}
